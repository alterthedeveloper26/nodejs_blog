<div class="mt-4">
  <div class="container">
    <h3>My Courses</h3>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Course Name</th>
          <th scope="col">Classification</th>
          <th scope="col" colspan="2">Created At</th>
        </tr>
      </thead>
      <tbody>
        {{#each courses}}
          <tr>
            <th scope="row">{{autoIncrease @index}}</th>
            <td>{{this.name}}</td>
            <td>{{this.classification}}</td>
            <td>{{this.createdAt}}</td>
            <td>
              <a href="/courses/{{this._id}}/edit" class="btn btn-dark">Edit</a>
              <a href="" class="btn btn-dark">Delete</a>
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>